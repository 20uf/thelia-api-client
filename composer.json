    "require": {
        "guzzle/guzzle": "~3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    },
    "authors": [
        {
            "name": "Benjamin Perche",
            "email": "bperche9@gmail.com"
        }
    ],
    "minimum-stability": "stable",
